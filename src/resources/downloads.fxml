<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" prefHeight="410" prefWidth="566" styleClass="downloads-grid-pane"
          stylesheets="stylesheet.css" xmlns="http://javafx.com/javafx/10.0.2-internal"
          maxHeight="410"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.DownloadsController">
    <children>
        <ScrollPane prefHeight="210.0" fx:id="scrollPane"
                    styleClass="downloads-scroll-pane" GridPane.halignment="CENTER"
                    GridPane.rowIndex="0" GridPane.rowSpan="1" GridPane.columnIndex="1"
                    GridPane.columnSpan="1" translateY="17"
                    GridPane.valignment="CENTER">
            <padding>
                <Insets left="1.0" right="1.0" top="1.0" bottom="1.0"/>
            </padding>
        </ScrollPane>
        <Button GridPane.halignment="CENTER"
                translateY="3"
                GridPane.columnSpan="1"
                GridPane.rowIndex="1" GridPane.rowSpan="1" GridPane.columnIndex="1"
                text="Close" styleClass="history-btn" onAction="#closePressed">

        </Button>
    </children>
    <rowConstraints>
        <RowConstraints percentHeight="79"/>
        <RowConstraints percentHeight="21"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints percentWidth="7"/>
        <ColumnConstraints percentWidth="86"/>
        <ColumnConstraints percentWidth="7"/>
    </columnConstraints>
</GridPane>
